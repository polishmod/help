{
  "name": "nebular-angular",
  "version": "1.0.0-dev",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.defdev.pl/angular.git"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --no-progress --proxy-config proxy.config.json",
    "generate": "node_modules/.bin/ng-swagger-gen --config ng-swagger-gen.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lintfix": "ng lint --fix",
    "e2e": "ng e2e",
    "proxy": "ng serve --proxy-config proxy.config.json",
    "document": "./node_modules/.bin/compodoc -p tsconfig.json",
    "restore": "npm install --suppess-warnings --loglevel=error",
    "container": "ng lint && ng build --configuration=container",
    "development": "ng lint && ng build --configuration=development",
    "staging": "ng lint && ng build --configuration=staging",
    "production": "ng lint && ng build --configuration=production",
    "swagger-gen": "node_modules/.bin/ng-swagger-gen --config ng-swagger-gen.json",
    "build-prod": "ng build --prod",
    "conventional-changelog": "conventional-changelog",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s"
  },
